<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xiaofei.shop.dao.RoleDao">

    <select id="getRoles" resultType="Role">
        select id,name
        from roles;
    </select>

    <select id="getRoleById" resultType="Role">
        select id,name
        from roles
        where id=#{id}
    </select>
</mapper>